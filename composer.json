{
    "name": "ruffle-labs/product-core",
    "description": "Product catalogue for your Laravel application",
    "type": "laravel-plugin",
    "license": "MIT",
    "keywords": ["laravel", "e-commerce"],
    "authors": [
        {
            "name": "Ben Clayton",
            "email": "Ben@benclayton.me"
        }
    ],
    "require": {
        "illuminate/support": "^5.4"
    },
    "require-dev": {
        "codeclimate/php-test-reporter": "^0.4.4",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7",
        "spatie/phpunit-watcher": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "RuffleLabs\\ProductCore\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RuffleLabs\\ProductCore\\Tests\\": "tests/"
        }
    }
}
